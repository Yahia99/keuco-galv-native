{
  "expo": {
    "name": "Keuco Galvanik App",
    "slug": "keuco-galv-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/fav.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "myapp",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000"
      }
    },
    "web": {
      "favicon": "./assets/fav.png",
      "bundler": "metro"
    },
    "plugins": ["expo-router"]
  }
}
